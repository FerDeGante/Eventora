    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.7.0",
    "@sentry/node": "^10.30.0",
    "@sentry/profiling-node": "^10.30.0",
    "@upstash/ratelimit": "^1.0.3",
    "@upstash/redis": "^1.34.3",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "fastify": "^4.28.1",
