{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "eventora",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/api",
    "apps/web"
  ],
  "scripts": {
    "dev": "npm run dev --workspace web",
    "dev:web": "npm run dev --workspace web",
    "dev:api": "npm run dev --workspace api",
    "build": "npm run build --workspace web",
    "build:web": "npm run build --workspace web",
    "build:api": "npm run build --workspace api",
    "start": "npm run start --workspace web",
    "start:web": "npm run start --workspace web",
    "start:api": "npm run start --workspace api",
    "lint": "npm run lint --workspace web",
    "migrate": "npm run migrate --workspace api",
    "generate": "npm run generate --workspace api",
    "seed": "npm run seed --workspace api",
    "test:rls": "tsx scripts/test-rls.ts",
    "db:reset": "bash scripts/db-reset.sh",
    "db:backup": "bash scripts/db-backup.sh",
    "db:restore": "bash scripts/db-restore.sh"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20"
  }
}
